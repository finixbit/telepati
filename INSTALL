


apt-get install libpcap-dev libopenssl1-dev libhiredis-dev libjansson-dev


git submodule add https://github.com/mfontanini/libtins.git third_party/libtins
git submodule add https://github.com/grpc/grpc.git third_party/grpc


$ cd third_party/libtins
$ mkdir build
$ cmake ../ -DLIBTINS_ENABLE_CXX11=1
$ make